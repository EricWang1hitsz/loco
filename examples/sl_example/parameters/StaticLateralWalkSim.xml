<?xml version="1.0" ?>
<LocomotionController>
	<Mission>
		<Speed>
			<Initial headingSpeed="0.0" lateralSpeed="0.0" turningSpeed="0.0" />
			<Maximum headingSpeed="1.0" lateralSpeed="0.3" turningSpeed="0.8" />
		</Speed>
	</Mission>
	<LimbCoordination>
		<GaitPattern> 
			<APS>
	             <GaitDiagram cycleDuration="3" foreLag="0.5" hindLag="0.5" pairLag="0.75" foreDutyFactor="0.77" hindDutyFactor="0.77" />
	        </APS>
		</GaitPattern>
	</LimbCoordination>
	<TorsoControl>
		<DynamicGait>
			<CoMOverSupportPolygonControl>
				<Weight minSwingLegWeight="0.2" />
				<Timing startShiftAwayFromLegAtStancePhase="0.5" startShiftTowardsLegAtSwingPhase="0.5" />
			</CoMOverSupportPolygonControl>
			<HipConfiguration>
				<HeightTrajectory offset="0.42" fore="true" hind="true">
					<Knot t="0.00" v="-0" />
					<Knot t="0.25" v="0.00" />
					<Knot t="0.50" v="-0.0" />
					<Knot t="0.75" v="0.00" />
					<Knot t="1.00" v="-0" />
				</HeightTrajectory>
			</HipConfiguration>
		</DynamicGait>
	</TorsoControl>
	<FootPlacementStrategy>
		<InvertedPendulum>
			<Offset>
				<Fore heading="-0.00" lateral="-0.01" />
				<Hind  heading="0.00" lateral="0.07" />
			</Offset>
			<HeightTrajectory>
				<Knot t="0.0" v="0.0" />
				<Knot t="0.5" v="0.07" />
				<Knot t="1.0" v="-0.00" />
			</HeightTrajectory>
	        <Gains feedbackScale="1.1" />
        </InvertedPendulum>
	</FootPlacementStrategy>
	<VirtualForceControl>
		<Gains>
			<Sagittal 	kp="500" 		kd="150" 	kff="25.0"	/>
			<Coronal 	kp="640" 	kd="100"	kff="0"		/>
			<Vertical 	kp="600" 	kd="120" 	kff="230"/>
			<Roll 		kp="400" 	kd="6" 	kff="0" />
			<Pitch 		kp="200" 	kd="9" 	kff="0" />
			<Yaw 		kp="0" 	kd="0" 	kff="0" /> 	/>
		</Gains>
	</VirtualForceControl>
	<ContactForceDistribution>
		<Weights>
			<Force Sagittal="1.0" Coronal="1.0" Vertical="0.1" />
			<Torque Roll="10" Pitch="10" Yaw="5" />
			<Regularizer value="0.00001"/>
		</Weights>
		<Constraints frictionCoefficient="0.7" />
	</ContactForceDistribution> 
</LocomotionController>


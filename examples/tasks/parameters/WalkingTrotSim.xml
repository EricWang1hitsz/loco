<?xml version="1.0" ?>
<LocomotionController>
	<Mission>
		<Speed>
			<Initial headingSpeed="0.0" lateralSpeed="0.0" turningSpeed="0.0" />
			<Maximum headingSpeed="1.0" lateralSpeed="0.3" turningSpeed="0.8" />
		</Speed>
	</Mission>
	<LimbCoordination>
		<GaitPattern> 
			<APS>
	            <GaitDiagram cycleDuration="0.8" foreDutyFactor="0.5" hindDutyFactor="0.5"  foreLag="0.5" hindLag="0.5" pairLag="0.5" />
	            <CycleDuration minVel="0.0" minVelValue="0.8" maxVel="0.8" maxVelValue="0.47" law="exp" />
	            <ForeDutyFactor minVel="0.0" minVelValue="0.5" maxVel="0.8" maxVelValue="0.4" law="exp" />
	            <HindDutyFactor minVel="0.0" minVelValue="0.5" maxVel="0.8" maxVelValue="0.4" law="exp" />
	            <PairLag minVel="0.0" minVelValue="0.5" maxVel="0.8" maxVelValue="0.5" law="lin" />
	        </APS>
		</GaitPattern>
	</LimbCoordination>
	<TorsoControl>
		<DynamicGait>
			<CoMOverSupportPolygonControl>
				<Weight minSwingLegWeight="0.15" />
				<Timing startShiftAwayFromLegAtStancePhase="0.7" startShiftTowardsLegAtSwingPhase="0.7" />
			</CoMOverSupportPolygonControl>
			<HipConfiguration>
				<HeightTrajectory offset="0.41" fore="true" hind="true">
					<Knot t="0.00" v="-0" />
					<Knot t="0.25" v="0.00" />
					<Knot t="0.50" v="-0.0" />
					<Knot t="0.75" v="0.00" />
					<Knot t="1.00" v="-0" />
				</HeightTrajectory>
			</HipConfiguration>
		</DynamicGait>
	</TorsoControl>
	<FootPlacementStrategy>
		<InvertedPendulum>
			<Offset>
				<Fore heading="-0.00" lateral="0.00" />
				<Hind  heading="0.00" lateral="0.00" />
			</Offset>
			<HeightTrajectory>
				<Knot t="0.0" v="0.0" />
				<Knot t="0.5" v="0.09" />
				<Knot t="1.0" v="-0.00" />
			</HeightTrajectory>
	        <Gains feedbackScale="1.1" />
        </InvertedPendulum>
	</FootPlacementStrategy>
	<VirtualModelController>
		<Gains>
			<Heading 	kp="0" 		kd="150" 	kff="60.0"	/>
			<Lateral 	kp="640" 	kd="100"	kff="0"		/>
			<Vertical 	kp="600" 	kd="120" 	kff="270"	/>
			<Roll 		kp="400" 	kd="6" 		kff="0" 	/>
			<Pitch 		kp="200" 	kd="9" 		kff="0" 	/>
			<Yaw 		kp="0" 		kd="100" 	kff="0" 	/>
		</Gains>
	</VirtualModelController>
	<ContactForceDistribution>
		<Weights>
			<Force heading="1.0" lateral="1.0" vertical="1.0" />
			<Torque roll="20" pitch="20" yaw="5" />
			<Regularizer value="0.00001"/>
		</Weights>
		<Constraints frictionCoefficient="0.7" minimalNormalForce="2.0"  />
	</ContactForceDistribution> 
</LocomotionController>

